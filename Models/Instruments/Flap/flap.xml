<?xml version="1.0"?>


<PropertyList>

	<path>flap.ac</path>
	<texture-path>flap.png</texture-path>
		
	
	 <animation>
  <type>material</type>
  <object-name>face</object-name>
  <object-name>needle-flap</object-name>
  <object-name>mount</object-name>
   <object-name>needle-slat</object-name>
   <object-name>needle-trim</object-name>
 <emission>
     <red>0.035</red>
     <green>0.012</green>
     <blue>0.007</blue>
    <factor-prop>controls/lighting/instruments-norm</factor-prop>
   </emission> 
 </animation>  

  <animation>
  <type>rotate</type>
  <object-name>needle-flap</object-name>
  <property>surface-positions/flap-pos-norm</property>
  <factor>90</factor>
  <center>
   <x-m>0.00203061</x-m>
   <y-m>0.00704753</y-m>
   <z-m>0.0123592</z-m> 	
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation> 

  <animation>
  <type>rotate</type>
  <object-name>needle-slat</object-name>
  <property>surface-positions/flap-pos-norm</property>
  <factor>90</factor>
  <center>
   <x-m>0.00202084</x-m>
   <y-m>-0.00848985</y-m>
   <z-m>0.0218831</z-m> 			
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation> 

   <animation>
  <type>rotate</type>
  <object-name>needle-trim</object-name>
<property>surface-positions/elevator-pos-norm</property> 
  <factor>90</factor>
  <center>
   <x-m>0.00199604</x-m>
   <y-m>-0.0186432</y-m>
   <z-m>-0.0126209</z-m> 			
		
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation> 

 <!-- *** LoD *** -->
 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>5</max-m>
 </animation>


</PropertyList>

